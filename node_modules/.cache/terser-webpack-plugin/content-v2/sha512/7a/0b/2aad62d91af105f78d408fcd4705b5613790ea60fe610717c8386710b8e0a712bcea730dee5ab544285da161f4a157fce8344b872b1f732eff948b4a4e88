ilter-form\">\r\n                {/* Select type */}\r\n                <div className=\"form-group\">\r\n                    <label htmlFor=\"type\">Property Type</label>\r\n                    <select name=\"type\" id=\"type\" value={type} className=\"form-control\" onChange={handleChange}>\r\n                        {types}\r\n                        {/* <option>All</option> */}\r\n                    </select>\r\n                </div>\r\n                {/* end of select type */}\r\n\r\n                {/* start of  Guests  filter */}\r\n                <div className=\"form-group\">\r\n                    <label htmlFor=\"capacity\">Work Desks Available</label>\r\n                    <select name=\"capacity\" id=\"capacity\" value={capacity} className=\"form-control\" onChange={handleChange}>\r\n                        {people}\r\n                        {/* <option>All</option> */}\r\n                    </select>\r\n                </div>\r\n                {/* end of Guests filter */}\r\n                {/* start of Location */}\r\n                <div className=\"form-group\">\r\n                    <label htmlFor=\"type\">Places(Worth escape)</label>\r\n                    <select name=\"place\" id=\"place\" value={place} className=\"form-control\" onChange={handleChange}>\r\n                        {places}\r\n                        {/* <option>All</option> */}\r\n                    </select>\r\n                </div>\r\n                {/* End of location filter */}\r\n\r\n                {/* start of  Price  filter */}\r\n                <div className=\"form-group\">\r\n                    <label htmlFor=\"price\">Price/per night - ₹{price}</label>\r\n                    <input type=\"range\" name=\"price\" min={minPrice} max={maxPrice} id =\"price\" value={price} onChange={handleChange} className=\"form-control\"/>\r\n                        \r\n                </div>\r\n                {/* end of Price filter */}\r\n\r\n                \r\n       